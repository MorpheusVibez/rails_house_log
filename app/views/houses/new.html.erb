<h1>Add Your House here</h1>
<%= form_for(@house) do |f| %>
  <%#= form_tag('/users')%>
  Location: <%= f.text_field :location, {class: 'test_class'} %><br/>

  Size: <%= f.number_field :size %><br/>

 <!-- Select the owner: <%= f.select :user_id, options_from_collection_for_select(@all_users, :id, :email), include_blank: true %><br/> -->

  
  Select the owner: <%= f.select :user_id, @all_users.collect {|u| [u.email, u.id] }, {prompt: 'Select'}%><br/>

  <%= f.submit("Create House")%>
<% end %>

